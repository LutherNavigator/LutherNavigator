{
  "name": "luthernavigator",
  "version": "0.1.0",
  "description": "The Luther Navigator project seeks to provide students a way to share their experiences abroad.",
  "main": "index.js",
  "scripts": {
    "build": "cd src && npm run build",
    "document": "npm run documentcode",
    "documentcode": "./node_modules/typedoc/bin/typedoc --out docs src/*.ts src/services src/routes --theme ./node_modules/typedoc-neo-theme/bin/default",
    "format": "cd src && npm run format",
    "formatcode": "cd src && npm run formatcode",
    "formatscripts": "cd src && npm run formatscripts",
    "start": "cd src && npm start",
    "postinstall": "cd src && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WKHAllen/LutherNavigator.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WKHAllen/LutherNavigator/issues"
  },
  "homepage": "https://github.com/WKHAllen/LutherNavigator#readme",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.19.2",
    "typedoc-neo-theme": "^1.0.10",
    "typescript": "^4.1.2"
  }
}
